{
  "name": "68kcounter",
  "displayName": "68k Counter",
  "description": "68000 ASM source code cycle counter",
  "version": "1.0.4",
  "author": "Graham Bates <hello@grahambates.com>",
  "publisher": "gigabates",
  "license": "MIT",
  "icon": "images/icon.png",
  "homepage": "https://github.com/grahambates/68kcounter-vscode.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/grahambates/68kcounter-vscode.git"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "activationEvents": [
    "onCommand:68kcounter.toggleCounts",
    "onCommand:68kcounter.countSelection",
    "onLanguage:m68k",
    "onLanguage:amiga-assembly-debug.disassembly",
    "onLanguage:amiga.assembly",
    "onLanguage:amiga.disassembly",
    "onLanguage:as",
    "onLanguage:vasmmot",
    "onLanguage:vasmstd",
    "onLanguage:codewarrior"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "68kcounter.toggleCounts",
        "title": "68kcounter: Toggle counts"
      },
      {
        "command": "68kcounter.countSelection",
        "title": "68kcounter: Count selection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "test": "node ./out/test/runTest.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "68kcounter": "^1.0.7"
  }
}
